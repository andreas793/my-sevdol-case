.layout__wrapper
    .main__img__wrapper
        img(src="./img/commercial-premises/commercial.png")
    .filters__wrapper
        .filters__wrapper__title Подбор коммерческого помещения
        .button.show-filter
            .icon__wrapper
                img(src="./img/commercial-premises/icons/filter.svg")
            .button__text Фильтр
            .button__counter 2
        .filters__form
            .close__filters__form
                img(src="./img/commercial-premises/icons/close.svg")
            .filters__wrapper__title Фильтр
            form(action="#").filters__form__content
                .filters__form__row
                    .filters__form__cell
                        .filters__form__label Объект
                        .object-name__checkbox
                            input(type="checkbox" name="object-name-sd" id="object-name-sd" value='object-name-sd')
                            input(type="checkbox" name="object-name-yount" id="object-name-yount" value='object-name-yount')
                            label(for="object-name-sd" class="object-name object-name-sd") Северная долина
                            label(for="object-name-yount" class="object-name object-name-yount") Ютолово
                    .filters__form__cell
                        .filters__form__label Стоимость аренды, ₽
                        .range__wrapper
                            input#input-with-keypress-0.range__input
                            input#input-with-keypress-1.range__input
                            #test-slider.range__trak
                    .filters__form__cell
                        .filters__form__label Площадь
                        .checkbox__section
                            .checkbox__wrapper
                                input(type='checkbox' id='square-1' name='square-1' value='square-1')
                                label(for='square-1') до 100 кв.м.
                            .checkbox__wrapper
                                input(type='checkbox' id='square-2' name='square-2' value='square-2')
                                label(for='square-2') 100-250 кв.м.
                            .checkbox__wrapper
                                input(type='checkbox' id='square-3' name='square-3' value='square-3')
                                label(for='square-3') 250-500 кв.м.
                            .checkbox__wrapper
                                input(type='checkbox' id='square-4' name='square-4' value='square-4')
                                label(for='square-4') более 500 кв.м.
                    .filters__form__cell.form__cell-small
                        .filters__form__label Очередь
                        select.select__wrapper
                            option(value="1") Неважно
                            option(value="2") 1
                            option(value="3") 2
                    .filters__form__cell.form__cell-small
                        .filters__form__label Корпус
                        select.select__wrapper
                            option(value="1") Неважно
                            option(value="2") 1
                            option(value="3") 2
                    .filters__form__cell.form__cell-normal
                        .filters__form__label Назначение
                        select.select__wrapper
                            option(value="1") Неважно
                            option(value="2") Ателье по пошиву одежды
                            option(value="3") Без функционального назначения
                            option(value="4") Помещение досуга и любительской деятельности
                            option(value="5") Салон красоты
                    .filters__form__cell.form__cell-normal
                        .filters__form__label Тестовый селект
                        select(name="fancySelect" class="makeMeFancy")
                            option(value="0") Выберите продукт
                            option( value="1") JavaScript + jQuery для начинающих в видеоформате
                            option( value="2") PHP + MySQL для начинающих
                            option( value="3") WordPress - профессиональный блог за один день
                            option( value="4") Joomla - профессиональный сайт за один день

                    .filters__actions__panel
                        button(class="button button-no-bg" type="reset") Очистить филтр
                        button(class="button button-black" type="submit") Подобрать помещение

    .result__serch
        .result__serch__title Результаты поиска
        select.js-choice
            option(value="1") Цена по возрастанию
            option(value="2") Цена по убыванию
            option(value="3") Площадь по возрастанию
            option(value="4") Площадь по убыванию
